{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfassignment12"
		},
		"Assignmentkiran_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Assignmentkiran'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"Duplicate_records_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Duplicate_records'"
		},
		"Grocery_distinct_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Grocery_distinct'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"grocery_data_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'grocery_data'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfassignment12_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Assignmentkiran_connectionString": {
						"value": "[parameters('Assignmentkiran_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"Duplicate_records_connectionString": {
						"value": "[parameters('Duplicate_records_connectionString')]"
					},
					"Grocery_distinct_connectionString": {
						"value": "[parameters('Grocery_distinct_connectionString')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"grocery_data_connectionString": {
						"value": "[parameters('grocery_data_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}